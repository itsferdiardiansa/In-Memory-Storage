{
  "name": "@zion/dm",
  "version": "1.0.0",
  "description": "A simple data storage. Built by using tree data structure",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/esm/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "show-config": "pnpm tsc --showConfig",
    "build": "rimraf dist && rollup -c",
    "dev": "NODE_ENV=development rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch --verbose=false",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.+(js|ts)\"",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --ignore-path .gitignore --check \"src/**/*.+(js|ts)\"",
    "format:write": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|ts)\""
  },
  "keywords": [
    "node",
    "typescript",
    "rollup",
    "storage",
    "data-structure"
  ],
  "private": "true",
  "author": "Ferdi Ardiansa",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "_moduleAliases": {
    "@root": "."
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/node": "^20.11.5",
    "module-alias": "^2.2.3",
    "ts-lib": "^0.0.5",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "@open-wc/building-rollup": "^3.0.2",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-run": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@zion/eslint-config": "workspace:^",
    "@zion/ts-config": "workspace:^",
    "deepmerge": "^4.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2"
  }
}
